	file = {Full Text PDF:/udd/abodrug/.zotero/zotero/0xbp7kyd.default/zotero/storage/DHGJXAUA/Zerbino and Birney - 2008 - Velvet Algorithms for de novo short read assembly.pdf:application/pdf;Snapshot:/udd/abodrug/.zotero/zotero/0xbp7kyd.default/zotero/storage/8ZGX7QRM/821.html:text/html}
